{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "python-stock-agent",
  "main": "src/entry.py",
  "compatibility_date": "2025-08-03",
  "compatibility_flags": ["python_workers"],
  "observability": {
    "enabled": true
  },
  
  "triggers": {
    "crons": [
      "0 */3 * * *"
    ]
  },
  
  "vars": {
    "ENVIRONMENT": "production"
  }
}